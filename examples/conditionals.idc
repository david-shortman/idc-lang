declare exclamation as "Boy-howdy!".

if exclamation is = "Boy-howdy!" then
    {
    displayln "Saddle up, partner!".
    }.

if exclamation is = "Boy-howdy" and 17 is = "seventeen" then
    {
    displayln "Nah, man".
    }.

if (12 + 10) is > 50 or exclamation is = "Sup" then
    {
    displayln "Probably not going to see me.".
    }
else then
    {
    displayln "Figured I'd see you here.".
    }.
